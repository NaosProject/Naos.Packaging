<!-- For usage of this file, see: https://msdn.microsoft.com/en-us/library/bb514188.aspx -->
<Dictionary>
  <Words>
    <Unrecognized>
      
      <!-- We prefer 'Dropdown' (or 'drop-down') to 'DropDown'.  See <Deprecated><Term PreferredAlternate="Dropdown"> for more details  -->
      <Word>DropDown</Word>

    </Unrecognized>
    <Recognized>
      <!-- Describes data that was made anonymous -->
      <Word>anonymized</Word>

      <!-- Application Programming Interface -->
      <Word>Api</Word>

      <!-- The name of the SAAS platform we use for continuous integration -->
      <Word>AppVeyor</Word>

      <!-- The name of the company whose Excel automation library we license -->
      <Word>Aspose</Word>
      
      <!-- A character encoding: American Standard Code for Information Interchange -->
      <Word>ascii</Word>

      <!-- A feature in Excel that automatically sizes rows and columns based on the contained content -->
      <Word>autofit</Word>

      <!-- Acronym for Amazon Web Services -->
      <Word>AWS</Word>

      <!-- Binary JSON -->
      <Word>Bson</Word>

      <!-- Describes a boolean -->
      <Word>bool</Word>

      <!-- Acronym for Cryptographic Message Syntax -->
      <Word>CMS</Word>

      <!-- The ability be be compiled. -->
      <Word>compilable</Word>

      <!-- Acronym for Certificate Signing Request -->
      <Word>CSR</Word>

      <!-- A piece of data. -->
      <Word>datapoint</Word>
      <Word>datapoints</Word>

      <!-- The act of deserializing. -->
      <Word>deserializes</Word>

      <!-- Refers to objects that are IEnumerable. -->
      <Word>enumerables</Word>

      <!-- Acronym for Extract, Transform, Load -->
      <Word>ETL</Word>

      <!-- Acronym for Fiscal Year -->
      <Word>FY</Word>

      <!-- Plural of Func, a .net type -->
      <Word>funcs</Word>

      <!-- A web-based hosting service for version control using git -->
      <Word>github</Word>

      <!-- Acronym for JavaScript Object Notation -->
      <Word>json</Word>

      <!-- The name of the SAAS platform we use for exploring log messages -->
      <Word>Loggly</Word>

      <!-- The name of an open source organization on GitHub, lead by Lawson Caudill -->
      <Word>Naos</Word>

      <!-- Represents a value Type in .NET that can be assigned null -->
      <Word>nullable</Word>

      <!-- The name of an open source organization on GitHub, lead by Suraj Gupta -->
      <Word>OBeautifulCode</Word>

      <!-- Acronym for OBeautifulCode, the name of an open source organization on GitHub, lead by Suraj Gupta -->
      <Word>OBC</Word>

      <!-- Synonym for parenthesis -->
      <Word>parens</Word>

      <!-- Acronym for Portable Document Format -->
      <Word>pdf</Word>

      <!-- 
      The de facto file format for storing and sending cryptographic keys, 
      certificates, and other data   
      -->
      <Word>PEM</Word>

      <!-- Describes something that persists information. -->
      <Word>persistor</Word>

      <!-- 
      Commonly-used to describe a PKCS#12 file, which stores a private key 
      and it's associated certificate chain. 
      -->
      <Word>PFX</Word>

      <!-- Acronym for Public Key Cryptography Standards -->
      <Word>PKCS</Word>

      <!-- Acronym for Request For Comments -->
      <Word>RFC</Word>

      <!-- Acronym for Secure Hash Algorithm. -->
      <Word>SHA</Word>

      <!-- A series of datapoints indexed in time order -->
      <Word>timeseries</Word>

      <!-- In .NET, a way describe a type parameter in generics. -->
      <Word>typeparam</Word>

      <!-- Refers to some piece of data that has not yet been validated -->
      <Word>unvalidated</Word>

      <!-- The file extension and file format for an Excel file -->
      <Word>xls</Word>
      <Word>xlsx</Word>

      <!-- The name of the SAAS platform we us for customer service support tickets, and help articles -->
      <Word>Zendesk</Word>
    </Recognized>
    <Deprecated>
      <!--
      Here we prefer a discrete term over the compound alternative.
      Unfortunately, because of a bug in Code Analysis, it's a little involved to trick the system into respecting our preference.
      See <Unrecognized><Word>...</Word></Unrecognized> section above
      See: https://stackoverflow.com/questions/2006795/fxcop-compound-word-should-be-treated-as-discrete-term
       -->

      <Term PreferredAlternate="Dropdown">DropDown</Term>

    </Deprecated>
    <Compound>
      <Term CompoundAlternate="CellPhone">cellphone</Term>
      <Term CompoundAlternate="OBeautifulCode">obeautifulcode</Term>
      <Term CompoundAlternate="RollUp">rollup</Term>
      <Term CompoundAlternate="YearEnd">yearend</Term>
    </Compound>
    <DiscreteExceptions>
      <Term></Term>
    </DiscreteExceptions>
  </Words>
  <Acronyms>
    <CasingExceptions>
      <Acronym>Co</Acronym>
      <Acronym>Etl</Acronym>
    </CasingExceptions>
  </Acronyms>
</Dictionary>
